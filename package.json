{
  "name": "@fulcrumapp/pg-query-deparser",
  "version": "0.0.1",
  "description": "Format PostgreSQL Queries from AST",
  "homepage": "http://github.com/fulcrumapp/pg-query-deparser",
  "main": "dist/index.js",
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "lint": "eslint src/* src/**/*",
    "build": "babel src --out-dir dist --source-maps",
    "watch": "babel src --out-dir dist --source-maps --watch",
    "clean": "rm -rf dist",
    "check-deps": "ncu",
    "update-deps": "ncu -u"
  },
  "repository": "git://github.com/fulcrumapp/pg-query-deparser.git",
  "eslintConfig": {
    "extends": "fulcrum",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "browserify": "^17.0.0",
    "chai": "^5.0.3",
    "eslint": "^8.57.0",
    "eslint-config-fulcrum": "^1.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.33.2",
    "glob": "^10.3.10",
    "mocha": "^10.3.0",
    "npm-check-updates": "^16.14.15",
    "source-map-support": "^0.5.21",
    "terser": "^5.28.1",
    "watch-cli": "^0.2.3",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "keywords": [
    "sql",
    "postgres",
    "postgresql",
    "pg",
    "query",
    "database"
  ]
}
